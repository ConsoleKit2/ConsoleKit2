msgid "pid must be > 1"
msgstr "pid трябва да е > 1"

#: src/ck-manager.c:3012
#, c-format
msgid "Unable to lookup session information for process '%d'"
msgstr "Невъзможно намиране на сесиина информация за процес '%d'"

